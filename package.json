    "cors": "^2.8.5",
    "express": "^4.18.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "simplebar": "^5.3.6",
    "sqlite3": "^5.0.8",
    "vue": "^3.2.33",
    "vue-simplebar": "^2.3.0",
